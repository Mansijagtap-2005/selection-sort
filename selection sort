#include<stdio.h>
int main(){
    //selaction sort
int i,a[5];
printf("enter array elements:\n");
for(int i=0;i<5;i++){
scanf("%d",&a[i]);
}
for(i=0;i<5;i++)//selects position
{
    for(int j=i+1;j<5;j++){
        if(a[i] > a[j]){
           int t=a[i];
           a[i]=a[j];
           a[j]=t;
        }
       
    }
   
}

 printf("Sorted Array is: \n");
    for(int i=0;i<5;i++){
        printf("%d\n",a[i]);
    }


}
